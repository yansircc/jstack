{
	"name": "jstack-app",
	"main": "src/server/index.ts",
	"compatibility_date": "2025-02-14",
	"observability": {
		"enabled": true
	},
	"placement": {
		"mode": "smart"
	},
	"dev": {
		"port": 8080
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "DB",
			"database_id": "YOUR_DATABASE_ID",
			"migrations_dir": "src/server/db/migrations"
		}
	],
	"vars": {
		"ENVIRONMENT": "development"
	}
}